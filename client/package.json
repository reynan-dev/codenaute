		"@graphql-codegen/cli": "^2.16.5",
		"@graphql-codegen/client-preset": "^1.3.0",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@testing-library/dom": "^8.20.0",
		"@testing-library/jest-dom": "^4.2.4",
