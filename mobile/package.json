{
	"name": "mobile",
	"version": "1.0.0",
	"private": true,
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"android": "expo start --android",
		"ios": "expo start --ios",
		"start": "concurrently \"tailwindcss -o styles/__generated__/tailwind.output.css --watch\" \"tailwindcss -i styles/__generated__/tailwind.output.css --postcss postcss.config.js\" \"npx expo\"",
		"start:clear": "concurrently \"tailwindcss -o styles/__generated__/tailwind.output.css --watch\" \"tailwindcss -i styles/__generated__/tailwind.output.css --postcss postcss.config.js\" \"npx expo -c\"",
		"web": "expo start --web"
	},
	"dependencies": {
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"expo": "~48.0.18",
		"expo-status-bar": "~1.4.4",
		"nativewind": "^2.0.0",
		"postcss": "^8.4.23",
		"react": "18.2.0",
		"react-native": "0.71.8",
		"tailwind-merge": "^1.14.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@types/react": "~18.0.14",
		"tailwindcss": "^3.3.2",
		"typescript": "^4.9.4"
	}
}
