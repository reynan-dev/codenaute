{
	"name": "mobile",
	"version": "1.0.0",
	"private": true,
	"main": "./src/index.ts",
	"scripts": {
		"android": "expo start --android",
		"codegen": "graphql-codegen --config codegen.ts",
		"generate:css": "tailwindcss -o src/styles/__generated__/tailwind.output.css --watch",
		"generate:nw": "tailwindcss -i src/styles/__generated__/tailwind.output.css --postcss postcss.config.js",
		"ios": "expo start --ios",
		"start": "npm run generate:css & npm run generate:nw > /dev/null 2>&1 & npx expo",
		"start:clear": "npm run generate:css & npm run generate:nw > /dev/null 2>&1 '[:print:]' & npx expo -c",
		"web": "npm run generate:css & npm run generate:nw > /dev/null 2>&1 & npx expo start --web"
	},
	"dependencies": {
		"@expo/webpack-config": "^18.0.1",
		"@react-native-async-storage/async-storage": "^1.19.1",
		"@react-navigation/bottom-tabs": "^6.5.8",
		"@react-navigation/native": "^6.1.7",
		"@react-navigation/native-stack": "^6.9.13",
		"autoprefixer": "^10.4.14",
		"cookie": "^0.5.0",
		"dotenv": "^16.3.1",
		"expo": "~48.0.18",
		"expo-constants": "~14.2.1",
		"expo-linking": "^5.0.2",
		"expo-status-bar": "~1.4.4",
		"nativewind": "2.0.0",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.71.8",
		"react-native-safe-area-context": "4.5.0",
		"react-native-web": "~0.18.10",
		"tailwind-merge": "^1.14.0"
	},
	"devDependencies": {
		"@apollo/client": "^3.7.17",
		"@babel/core": "^7.20.0",
		"@graphql-codegen/cli": "^4.0.1",
		"@graphql-codegen/client-preset": "^4.0.1",
		"@types/cookie": "^0.5.1",
		"@types/react": "~18.0.14",
		"tailwindcss": "3.3.2",
		"typescript": "^4.9.4"
	}
}
