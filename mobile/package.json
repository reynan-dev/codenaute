{
	"name": "mobile",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"android": "expo start --android",
		"generate:css": "tailwindcss -o style/__generated__/tailwind.output.css --watch",
		"generate:nw": "tailwindcss -i style/__generated__/tailwind.output.css --postcss postcss.config.js",
		"ios": "expo start --ios",
		"start": "concurrently \"pnpm generate:css\" \"pnpm generate:nw\" \"expo start\"",
		"start:clear": "concurrently \"pnpm generate:css\" \"pnpm generate:nw\" \"expo start -c\"",
		"start:watch": "concurrently \"pnpm generate:css\" \"pnpm generate:nw\" \"expo start -w\"",
		"test": "jest --watchAll",
		"web": "expo start --web"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@apollo/client": "^3.7.17",
		"@babel/runtime": "^7.22.6",
		"@expo/vector-icons": "^13.0.0",
		"@react-navigation/native": "^6.0.2",
		"@rnx-kit/metro-config": "^1.3.7",
		"@rnx-kit/metro-resolver-symlinks": "^0.1.29",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"expo": "~48.0.18",
		"expo-font": "~11.1.1",
		"expo-linking": "~4.0.1",
		"expo-router": "^1.5.2",
		"expo-splash-screen": "~0.18.2",
		"expo-status-bar": "~1.4.4",
		"expo-system-ui": "~2.2.1",
		"expo-web-browser": "~12.1.1",
		"graphql": "^16.7.1",
		"metro-runtime": "^0.77.0",
		"nativewind": "^2.0.11",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.71.8",
		"react-native-gesture-handler": "~2.9.0",
		"react-native-safe-area-context": "4.5.0",
		"react-native-screens": "~3.20.0",
		"react-native-web": "~0.18.10",
		"react-toastify": "^9.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@graphql-codegen/cli": "^4.0.1",
		"@graphql-codegen/client-preset": "^4.0.1",
		"@types/node": "^20.4.2",
		"@types/react": "~18.0.38",
		"@types/react-dom": "^18.0.11",
		"jest": "^29.2.1",
		"jest-expo": "~48.0.0",
		"react-test-renderer": "18.2.0",
		"tailwindcss": "3.3.2",
		"typescript": "^4.9.4"
	},
	"pnpm": {
		"overrides": {
			"xml2js@<0.5.0": ">=0.5.0",
			"json5@<1.0.2": ">=1.0.2",
			"semver@>=7.0.0 <7.5.2": ">=7.5.2"
		}
	}
}
