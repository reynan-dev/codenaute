{
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"devCommand": "npm run build",
	"startCommand": "npm run start",
	"functions": {
		"src/index.ts": {
			"includeFiles": ["src/**/*", "dist/**/*"],
			"runtime": "@vercel/node"
		}
	},
	"routes": [
		{
			"src": "(.*)",
			"dest": "src/index.ts"
		}
	]
}
