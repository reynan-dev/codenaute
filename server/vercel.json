{
	"version": 2,
	"buildCommand": "pnpm run build",
	"outputDirectory": "dist",
	"trailingSlash": false,
	"builds": [
		{
			"src": "src/index.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["src/**/*", "dist/**/*"]
			}
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "src/index.ts"
		}
	]
}
